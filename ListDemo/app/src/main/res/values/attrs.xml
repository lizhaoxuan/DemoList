<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!--  用于个人信息页面的自定义控件 -->
    <declare-styleable name="MapItem">
        <attr name="keyText" format="string" />
        <attr name="valueText" format="string" />
    </declare-styleable>

    <declare-styleable name="ProgressBarItem">
        <attr name="barName" format="string" />
    </declare-styleable>

    <declare-styleable name="TopToast">
        <attr name="android:text" />
    </declare-styleable>

    <declare-styleable name="NoDataTips">
        <attr name="android:text" />
    </declare-styleable>

    <declare-styleable name="ScrollBanner">
        <attr name="height" format="dimension"/>
        <attr name="orientation">
            <enum name="horizontal" value="1" />
            <enum name="vertical" value="0" />
        </attr>
    </declare-styleable>
</resources>